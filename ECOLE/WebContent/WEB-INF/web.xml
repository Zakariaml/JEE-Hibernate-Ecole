<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ECOLE</display-name>
  
  <servlet>
  	<servlet-name>ModuleServlet</servlet-name>
  	<servlet-class>Controller.ModuleServletController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ModuleServlet</servlet-name>
  	<url-pattern>/Modules</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ModuleServlet</servlet-name>
  	<url-pattern>*.aspx</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>EleveServlet</servlet-name>
  	<servlet-class>Controller.EleveServletController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>EleveServlet</servlet-name>
  	<url-pattern>/Eleve</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EleveServlet</servlet-name>
  	<url-pattern>*.php</url-pattern>
  </servlet-mapping>
  
   
  <servlet>
  	<servlet-name>NoteServlet</servlet-name>
  	<servlet-class>Controller.NoteServletController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>NoteServlet</servlet-name>
  	<url-pattern>/Note</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>NoteServlet</servlet-name>
  	<url-pattern>*.html</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>